{
  "name": "@sdods/core",
  "version": "0.1.0",
  "description": "Core utilities and types for SDODS",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "clean": "rm -rf dist"
  },
  "devDependencies": {
    "tsup": "^8.0.0",
    "typescript": "^5.4.0",
    "vitest": "^2.0.0"
  },
  "keywords": ["sdods", "utilities", "typescript"],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/siri1410/sdods.git",
    "directory": "packages/core"
  },
  "publishConfig": {
    "access": "public"
  }
}
